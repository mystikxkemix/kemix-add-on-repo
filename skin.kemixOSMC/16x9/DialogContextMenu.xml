<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- contextmenu -->
	<defaultcontrol always="true">996</defaultcontrol>
	<coordinates>
		<origin x="0" y="0"/>
	</coordinates>
	<controls>
		<!-- Background -->
		<include>DialogBackgroundImage</include>
		<!-- Default grouplist -->
		<control type="grouplist" id="996">
			<centerleft>50%</centerleft>
			<width>440</width>
			<height>auto</height>
			<onup>noop</onup>
			<ondown>noop</ondown>
			<itemgap>0</itemgap>
			<align>center</align>
			<include>DialogAnimation</include>
		</control>
		<!-- Default button -->
		<control type="button" id="1000">
			<width>440</width>
			<height>68</height>
			<align>center</align>
			<font>Med</font>
			<texturefocus colordiffuse="DialogBackgroundColor">common/white.png</texturefocus>
			<texturenofocus colordiffuse="DialogBackgroundColor">common/white.png</texturenofocus>
		</control>
	</controls>
</window>